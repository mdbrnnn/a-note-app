{
    "version": 2,
    "builds": [
      {
        "src": "frontend/index.html",
        "use": "@vercel/static"
      },
      {
        "src": "backend/api/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/frontend/$1" },
      { "src": "/api/(.*)", "dest": "/backend/api/$1" }
    ]
  }
  